{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "functions": {
    "api/index.php": {
      "runtime": "php-8.4"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"]
    }
  ]
}
